{
  "name": "Le parisien ad-blocker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Allow users to read Le pari",
  "content_scripts": [
    {
      "matches": ["http://www.leparisien.fr/**/*", "https://www.leparisien.fr/**/*"],
      "js": ["inject.js"],
      "run_at": "document_idle"
    }
  ],
  "browser_action": {
    "default_title": "Le parisien ad-blocker"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}